msgid "Active"
msgstr "アクティブ"

msgid "Available"
msgstr "利用可能"

msgid "Capacity"
msgstr "容量"

msgid "Create"
msgstr "作成"

msgid "Created logical volume '{{ name }}'."
msgstr "論理ボリューム '{{ name }}' を作成しました。"

msgid "Created physical volume {{ devicefile }}."
msgstr "物理ボリューム {{ devicefile }} を作成しました。"

msgid "Created snapshot of logical volume '{{ devicefile }}'."
msgstr "論理ボリューム '{{ devicefile }}' のスナップショットを作成しました。"

msgid "Created volume group '{{ name }}'."
msgstr "ボリュームグループ '{{ name }}' を作成しました。"

msgid "Device"
msgstr "デバイス"

msgid "Devices"
msgstr "デバイス"

msgid "Do you really want to create a physical volume on '{{ devicefile }}'?"
msgstr "'{{devicefile}}'に物理ボリュームを作成しますか？"

msgid "Do you really want to resize the selected physical volume?"
msgstr "選択した物理ボリュームをリサイズしてもよろしいですか？"

msgid "Edit"
msgstr "編集"

msgid "Extend"
msgstr "拡張"

msgid "Extended logical volume '{{ devicefile }}'."
msgstr "拡張論理ボリューム '{{devicefile}}'。"

msgid "Extended volume group '{{ devicefile }}'."
msgstr "拡張ボリュームグループ '{{devicefile}}'。"

msgid "Free"
msgstr "空き"

msgid "Invalid name"
msgstr "不正な名前"

