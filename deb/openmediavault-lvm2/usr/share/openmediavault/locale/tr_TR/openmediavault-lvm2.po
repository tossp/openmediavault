msgid "Active"
msgstr "Aktif"

msgid "Available"
msgstr "Hazır"

msgid "Capacity"
msgstr "Kapasite"

msgid "Create"
msgstr "Oluştur"

msgid "Created logical volume '{{ name }}'."
msgstr "Oluşturulmuş mantıksal bölüm '{{ name }}'."

msgid "Created physical volume {{ devicefile }}."
msgstr "Oluşturulmuş fiziksel bölüm {{ devicefile }}."

msgid "Created snapshot of logical volume '{{ devicefile }}'."
msgstr "Mantıksal bölüm '{{ devicefile }}' ın oluşturulmuş görüntüsü."

msgid "Created volume group '{{ name }}'."
msgstr "Oluşturulmuş bölüm grubu '{{ name }}'."

msgid "Device"
msgstr "Aygıt"

msgid "Devices"
msgstr "Aygıtlar"

msgid "Do you really want to create a physical volume on '{{ devicefile }}'?"
msgstr "'{{ devicefile }}' üzerinde fiziksel bir bölüm oluşturmak istediğnize emin misiniz?"

msgid "Do you really want to resize the selected physical volume?"
msgstr "Seçilen fiziksel bölümü yeniden boyutlandırmak istediğinize emin misiniz?"

msgid "Edit"
msgstr "Düzenle"

msgid "Extend"
msgstr "Genişlet"

msgid "Extended logical volume '{{ devicefile }}'."
msgstr "Genişletilmiş mantıksal bölüm '{{ devicefile }}'."

msgid "Extended volume group '{{ devicefile }}'."
msgstr "Genişletilmiş bölüm grubu '{{ devicefile }}'."

msgid "Free"
msgstr "Boş"

msgid "Invalid name"
msgstr "Geçersiz ad"

msgid "Logical Volume Management"
msgstr "Mantıksal Bölüm Yönetimi"

msgid "Logical Volumes"
msgstr "Mantıksal bölümler"

msgid "Name"
msgstr "İsim"

msgid "Physical Volumes"
msgstr "Fiziksel Bölümler"

msgid "Reduce"
msgstr "Küçült"

msgid "Reduced volume group '{{ devicefile }}'."
msgstr "Ufaltılmış bölüm grubu '{{ devicefile }}'."

msgid "Resize"
msgstr "Yeniden boyutlandır"

msgid "Select a device ..."
msgstr "Aygıt seçiniz ..."

msgid "Select a volume group ..."
msgstr "Bölüm grubu seç ..."

msgid "Select devices ..."
msgstr "Aygıtları seçiniz ..."

msgid "Select the devices used to extend the volume group."
msgstr "Bölüm grubunu genişletmek için kullanılacak aygıtları seçiniz."

msgid "Select the devices used to reduce from the volume group."
msgstr "Bölüm grubunu küçültmek için kullanılacak aygıtları seçiniz."

msgid "Size"
msgstr "Büyüklük"

msgid "Snapshot"
msgstr "Anlık Durum Görüntüsü"

msgid "The name of the logical volume."
msgstr "Mantıksal bölüm adı."

msgid "The name of the volume group."
msgstr "Bölüm grubu adı."

msgid ""
"The percentage of the total space in the volume group to allocate for the "
"new logical volume."
msgstr "Yeni mantıksal bölüm için kullanılacak alanın toplam bölüm alanı içindeki yüzdesi."

msgid ""
"The percentage of the total space in the volume group to use by the logical "
"volume."
msgstr "Bölüm grubunda mantıksal bölüm tarafından kullanılan tğm alanın yüzdesi."

msgid "Updated logical volume '{{ devicefile }}'."
msgstr "Güncellenmiş mantıksal bölüm '{{ devicefile }}'."

msgid "Updated volume group '{{ devicefile }}'."
msgstr "Güncellenmiş bölüm grubu '{{ devicefile }}'."

msgid "Used"
msgstr "Kullanılan"

msgid "Volume Group"
msgstr "Bölüm Grubu"

msgid "Volume Groups"
msgstr "Bölüm Grupları"

msgid "Volume group"
msgstr "Bölüm grubu"
