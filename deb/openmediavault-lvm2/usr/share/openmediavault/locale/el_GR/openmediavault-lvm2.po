msgid "Active"
msgstr "Ενεργό"

msgid "Available"
msgstr "Διαθέσιμο"

msgid "Capacity"
msgstr "Χωρητικότητα"

msgid "Create"
msgstr "Δημιουργία"

msgid "Created logical volume '{{ name }}'."
msgstr "Δημιουργία λογικού τόμου '{{ name }}'."

msgid "Created physical volume {{ devicefile }}."
msgstr "Δημιουργία φυσικού τόμου '{{ devicefile }}'."

msgid "Created snapshot of logical volume '{{ devicefile }}'."
msgstr "Δημιουργία στιγμιότυπου του λογικού τόμου '{{ devicefile }}'."

msgid "Created volume group '{{ name }}'."
msgstr "Δημιουργία ομάδας τόμων '{{ name }}'."

msgid "Device"
msgstr "Συσκευή"

