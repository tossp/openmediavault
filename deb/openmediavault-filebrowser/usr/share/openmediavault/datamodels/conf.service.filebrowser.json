{
	"type": "config",
	"id": "conf.service.filebrowser",
	"title": "File Browser",
	"queryinfo": {
		"xpath": "//services/filebrowser",
		"iterable": false
	},
	"properties": {
		"enable": {
			"type": "boolean",
			"default": false
		},
		"port": {
			"type": "integer",
			"minimum": 1,
			"maximum": 65535,
			"default": 3670
		},
		"sslcertificateref": {
			"type": "string",
			"oneOf": [{
				"type": "string",
				"format": "uuidv4"
			},{
				"type": "string",
				"maxLength": 0
			}]
		},
		"sharedfolderref": {
			"type": "string",
			"oneOf": [{
				"type": "string",
				"format": "uuidv4"
			},{
				"type": "string",
				"maxLength": 0
			}]
		}
	}
}
